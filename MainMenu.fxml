<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.MainMenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="customerRecords" layoutX="235.0" layoutY="88.0" mnemonicParsing="false" onAction="#clickCustomer" text="Customer Records">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="logOff" layoutX="559.0" layoutY="355.0" mnemonicParsing="false" onAction="#clickLogOff" text="Log off" />
            <Button fx:id="appointmentPage" layoutX="226.0" layoutY="176.0" mnemonicParsing="false" onAction="#clickAppt" prefHeight="31.0" prefWidth="159.0" text="Appointment Page">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Text layoutX="215.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Main Menu" underline="true">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
            <Button fx:id="reportsPage" layoutX="252.0" layoutY="258.0" mnemonicParsing="false" onAction="#clickReports" text="Reports Page" />
         </children>
    </AnchorPane>
  </children>
</VBox>
