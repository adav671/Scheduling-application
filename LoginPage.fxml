<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.LoginPageController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="368.0" prefWidth="541.0" VBox.vgrow="ALWAYS">
      <children>
            <PasswordField fx:id="passwordLabel" layoutX="242.0" layoutY="189.0" />
            <Label fx:id="passwordField" layoutX="102.0" layoutY="194.0" text="Password :" />
            <Label fx:id="usernameField" layoutX="102.0" layoutY="137.0" text="Username :" />
            <Label fx:id="signOn" layoutX="177.0" prefHeight="128.0" prefWidth="400.0" text="Sign On" textAlignment="CENTER" textOverrun="CLIP" underline="true">
               <font>
                  <Font name="System Bold Italic" size="72.0" />
               </font>
            </Label>
            <Button fx:id="exitLogIn" layoutX="582.0" layoutY="323.0" mnemonicParsing="false" onAction="#clickExitLogIn" text="Exit" />
            <Button fx:id="logIn" layoutX="320.0" layoutY="245.0" mnemonicParsing="false" onAction="#clickLogIn" text="Login" />
            <TextField fx:id="userLabel" layoutX="242.0" layoutY="132.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
